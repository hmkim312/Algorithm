     "shell.execute_reply.started": "2020-10-10T14:51:11.395051Z"
    }
   },
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    factor = 0\n",
    "    for i in range(1, n + 1):\n",
    "        if (n) % i == 0:\n",
    "            factor += i\n",
    "    return factor"
   ]
  },
